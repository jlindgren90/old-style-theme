[%General]
author=Tsu Jan
comment=A dark theme with a Gnome like look and feel
attach_active_tab=false
button_icon_size=24
center_toolbar_handle=true
check_size=18
combo_focus_rect=true
combo_menu=true
dialog_button_layout=2
hide_combo_checkboxes=true
large_icon_size=48
layout_spacing=7
left_tabs=true
mirror_doc_tabs=false
no_inactiveness=true
progressbar_thickness=4
scroll_arrows=false
scrollbar_in_view=true
slider_handle_length=24
slider_handle_width=26
slider_width=4
small_icon_size=24
spin_button_width=28
spread_progressbar=true
submenu_overlap=1
textless_progressbar=false
tickless_slider_handle_size=24
toolbar_icon_size=24
toolbar_interior_spacing=3
toolbutton_style=1

[GeneralColors]
dark.color=#111111
highlight.color=#15539e
alt.base.color=#1b1b1b
base.color=#222222
button.color=#333333
window.color=#333333
mid.color=#444444
mid.light.color=#555555
disabled.text.color=#5d5d5d
light.color=#666666
button.text.color=#eeeeee
text.color=#eeeeee
tooltip.text.color=#eeeeee
window.text.color=#eeeeee
highlight.text.color=white

[PanelButtonCommand]
frame=true
frame.element=btn
frame.expandedElement=button
frame.expansion=12
frame.left=3
frame.right=3
frame.top=2
frame.bottom=4
indicator.element=arrow
indicator.size=14
interior=true
interior.element=button
text.iconspacing=4
text.margin=true
text.margin.left=2
text.margin.right=2
text.margin.top=2
text.margin.bottom=2
text.focus.color=#eeeeee
text.normal.color=#eeeeee
text.press.color=#eeeeee
text.toggle.color=#eeeeee

[PanelButtonTool]
inherits=PanelButtonCommand
frame.element=btn
frame.expandedElement=button

[ToolbarButton]
inherits=PanelButtonCommand
frame.element=tbtn
frame.expandedElement=tbutton
interior.element=tbutton

[Dock]
